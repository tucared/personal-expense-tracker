{
  "mcpServers": {
    "duckdb-prod": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-motherduck",
        "--db-path",
        "prod.duckdb",
        "--read-only"
      ]
    },
    "duckdb-dev": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-motherduck",
        "--db-path",
        "dev.duckdb",
        "--read-only"
      ]
    },
    "duckdb-local": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-motherduck",
        "--db-path",
        ":memory:"
      ]
    }
  }
}
